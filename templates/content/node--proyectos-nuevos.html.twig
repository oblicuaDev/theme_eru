{# /** * @file * Template for a Node. */ #} {% set classes = [ 'node',
node.isPromoted() ? 'node--promoted', node.isSticky() ? 'node--sticky', not
node.isPublished() ? 'node--unpublished', node.bundle|clean_class,
node.bundle|clean_class ~ '--' ~ view_mode|clean_class, 'project' ] %} {#
{{ kint(content | keys) }} #}
<article {{ attributes.addClass(classes) }}>
  {% block content %}
  <div class="container container-project-new mt-5">
 
    <ul class="nav nav-pills nav-fill" id="myTab" role="tablist">
      <li class="nav-item">
        <a aria-controls="description" aria-selected="true" class="nav-link active" data-toggle="tab" href="#description" id="description-tab" role="tab">Descripción</a>
      </li>
      <li class="nav-item">
        <a aria-controls="benefits" aria-selected="false" class="nav-link" data-toggle="tab" href="#benefits" id="benefits-tab" role="tab">Beneficios</a>
      </li>
      <li class="nav-item">
        <a aria-controls="scope" aria-selected="false" class="nav-link" data-toggle="tab" href="#scope" id="scope-tab" role="tab">Alcance</a>
      </li>
      <li class="nav-item">
        <a aria-controls="status" aria-selected="false" class="nav-link" data-toggle="tab" href="#benefits" id="benefits-tab" role="tab">Estado e Inversión</a>
      </li>
      <li class="nav-item">
        <a aria-controls="multimedia" aria-selected="false" class="nav-link" data-toggle="tab" href="#photos" id="photos-tab" role="tab">Multimedia</a>
      </li>
     
    </ul>
    <div class="tab-content" id="myTabContent">
      <div aria-labelledby="description-tab" class="tab-pane fade show active project-content" id="description" role="tabpanel">
        {{ content.body }}
        {{content.field_location}}
      </div>
      <div aria-labelledby="benefits-tab" class="tab-pane fade project-content" id="benefits" role="tabpanel">

      </div>
      <div aria-labelledby="scope-tab" class="tab-pane fade project-content row" id="scope" role="tabpanel">

      </div>
      <div aria-labelledby="status-tab" class="tab-pane fade project-content" id="status" role="tabpanel">

      </div>
      <div aria-labelledby="multimedia-tab" class="tab-pane fade row project-content" id="multimedia" role="tabpanel">

      </div>
    </div>
  </div>
  {% endblock %}
</article>