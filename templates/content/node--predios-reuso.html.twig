{#
/**
 * @file
 * Template for a Node.
 */
#}
{%
  set classes = [
  'node',
  node.isPromoted() ? 'node--promoted',
  node.isSticky() ? 'node--sticky',
  not node.isPublished() ? 'node--unpublished',
  node.bundle|clean_class,
  node.bundle|clean_class ~ '--' ~ view_mode|clean_class,
  'normatividad'
]
%}
{% set createdDate = node.getCreatedTime|date('j/m/Y - H:i') %}
{# {{ kint(content|keys)}} #}
<article{{attributes.addClass(classes)}}>
	{% block content %}
		<div class="container">
			{{content.field_image_main}}
			{{content.field_barrio_catastral}}
			{{content.field_ano_de_construccion}}
			{{content.field_upl}}
			{{content.field_bic}}
			{{content.field_no_de_unidades_estimadas}}
			{{content.field_ventas_estimadas}}
			{{content.field_a}}
			{{content.field_area_suelo}}
			{{content.field_no_de_pisos}}
		</div>
	{% endblock %}
</article>
