    {%
      set classes = [
        'paragraph',
        'paragraph--type--' ~ paragraph.bundle|clean_class,
        view_mode ? 'paragraph--view-mode--' ~ view_mode|clean_class,
      ]
    %}
    
    <div{{ attributes.addClass(classes) }}>
        {{ content.field_nombre_contratista_o_inter.value }}
        {{ content.field_numero_de_contrato.value }}
        {{ content.field_estado_del_contrato.value }}
        {{ content.field_documento.value }}
    </div>