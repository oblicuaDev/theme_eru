    {%
      set classes = [
        'paragraph',
        'paragraph--type--' ~ paragraph.bundle|clean_class,
        view_mode ? 'paragraph--view-mode--' ~ view_mode|clean_class,
      ]
    %}
    
    <div{{ attributes.addClass(classes) }}>
        {{ content.field_image }}
        {{ content.field_figcaption }}
        {{ content.field_nombre_contratista_o_inter }}
        {{ content.field_numero_de_contrato }}
        {{ content.field_estado_del_contrato }}
        {{ content.field_documento }}
    </div>