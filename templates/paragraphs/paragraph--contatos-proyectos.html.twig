{%
set classes = [
'paragraph',
'paragraph--type--' ~ paragraph.bundle|clean_class,
view_mode ? 'paragraph--view-mode--' ~ view_mode|clean_class,
]
%}

<div{{ attributes.addClass(classes) }}>
    {{ dump(content.field_contratos) }}
    {% for key, item in content.field_contratos %}
    {{ dump(item) }}
    <div class="name">
        {{ item.field_nombre_contratista_o_inter }}
    </div>
    <div class="data">
        <div class="number">
            <div class="icon">
                <img src="/sites/default/files/2023-11/numero.png" alt="icon">
            </div>
            <div class="txt">
                <strong>NÃºmero de contrato</strong>
                {{ item.field_numero_de_contrato }}
            </div>
        </div>
        <div class="estado">
            <div class="icon">
                <img src="/sites/default/files/2023-11/estadoPro.png" alt="icon">
            </div>
            <div class="txt">
                <strong>Estado del contrato</strong>
                {{ item.field_estado_del_contrato }}
            </div>
        </div>
        <div class="documento">
            <div class="icon">
                <img src="/sites/default/files/2023-11/link.png" alt="icon">
            </div>
            <div class="txt">
                <strong>Documento contractua</strong>
                <a href="{{ item.field_documento }}">Link Secop</a>
            </div>
        </div>
    </div>
    {% endfor %}
    </div>