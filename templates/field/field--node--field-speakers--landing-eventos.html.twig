{#
    /**
     * @file
     * Template for a field.
     */
    #} 
   
      {% if multiple %}
        <div{{ attributes }}>
          {% for item in items %}
          {{ item.content['#node'].field_foto.value|raw }}
          {{ item.content['#node'].field_nombre.value|raw }}
          {{ item.content['#node'].field_descrip.value|raw }}
          {% endfor %}
        </div>
      {% else %}
        {% for item in items %}
        {{ item.content['#node'].field_foto.value|raw }}
        {{ item.content['#node'].field_nombre.value|raw }}
        {{ item.content['#node'].field_descrip.value|raw }}
        {% endfor %}
      {% endif %}
